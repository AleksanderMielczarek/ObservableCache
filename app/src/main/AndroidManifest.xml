<manifest package="com.github.aleksandermielczarek.observablecacheexample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".ObservableCacheApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.Observable1Activity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ui.Observable2Activity_"/>
    </application>

</manifest>
